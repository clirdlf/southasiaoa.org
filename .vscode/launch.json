{
  "name": "11ty",
  "type": "node",
  "request": "launch",
  "program": "${workspaceRoot}/node_modules/.bin/eleventy",
  "stopOnEntry": false,
  "args": ["--watch"],
  "cwd": "${workspaceRoot}",
  "console": "internalConsole",
  "configurations": [
    {
      "command": "npm start",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
